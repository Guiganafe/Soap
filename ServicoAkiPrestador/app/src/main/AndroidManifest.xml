<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.justworks.prestador.ServicoAki">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.EditarEvento"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".Activity.ConfigurarAgenda"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.EditarEndereco"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.AdicionarEndereco"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.MeusEnderecos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.SelecionarEvento"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.EditarServico"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.EditarPerfil"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.AdicionarServico"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.MeusServicos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activity.Services" />
        <activity
            android:name=".Activity.CadastroActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.CriarEvento"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.CompletarCadastroActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="br.com.justworks.android.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>
    </application>

</manifest>